    // Webサービス用
    "Azure": "ASPNETWebServiceCore",
    "FxContainerization": "ON",

    // フレームワークの使用するパラメータ - start

    // 共有情報定義へのパス
    "FxXMLSPDefinition": "ASPNETWebServiceCore.Xml.SPDefinition.xml",
    // メッセージ定義へのパス
    "FxXMLMSGDefinition": "ASPNETWebServiceCore.Xml.MSGDefinition.xml",
